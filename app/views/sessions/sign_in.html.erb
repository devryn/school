<h2>Sign In</h2>
<p class="field_has_errors"><%= flash[:alert] %></p>
<%= form_tag '/sessions/authenticate', method: "POST" do %>
  <%= label_tag :email %>
  <%= email_field_tag :email %>

  <%= label_tag :password %>
  <%= password_field_tag :password %>

  <div class='actions'>
    <%= submit_tag "Submit" %>
  </div>
<% end %>
